{
  "kind": "collectionType",
  "collectionName": "course_instances",
  "info": {
    "singularName": "course-instance",
    "pluralName": "course-instances",
    "displayName": "course-instance",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "course_instances"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "dateFrom": {
      "type": "date"
    },
    "dateTo": {
      "type": "date"
    },
    "lessons": {
      "type": "component",
      "repeatable": true,
      "component": "course-structure.lesson",
      "required": true
    },
    "quizzes": {
      "type": "component",
      "repeatable": true,
      "component": "exam-structure.quiz"
    },
    "teacher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "teacherFor"
    },
    "students": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "courses"
    },
    "numberOfStudetn": {
      "type": "integer",
      "required": true
    }
  }
}
